@using (Html.BeginForm("SelectMain", "ConsultaResultados", FormMethod.Post, new { id = "frmInstituciones" }))
{
    <fieldset class="default">
        <legend class="default">Búsqueda de Resultados Online:</legend>
        <div class="form-group">
            <div class="row">
                <div class="col-sm-2 divWidth">
                    <div class="input-group radioButtonWidthReg">
