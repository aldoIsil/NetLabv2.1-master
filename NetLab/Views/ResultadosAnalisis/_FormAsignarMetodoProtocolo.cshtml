<script src="~/Scripts/JsExcel/xlsx.core.min.js" type="text/javascript"></script>
<script src="~/Scripts/JsExcel/xls.core.min.js" type="text/javascript"></script>

<fieldset class="default">
    <legend class="default">Cargar Muestras SARS</legend>
    <div id="dvBusquedaUsuario2">
        <div class="form-group">
            <div class="row">
                @Html.Label("Archivo: ", new { @class = " -label col-xs-12 col-sm-3 col-md-2 col-lg-2" })
                <div class="col-xs-12 col-sm-8 col-md-3 col-lg-3">
                    <input type="file" id="excelfile" onchange="handleFiles(this.files)" accept=".xlsx" />
                </div>
            </div>
            <br />
            <div class="row">
                @Html.Label("Metodo/Kit: ", new { @class = " -label col-xs-12 col-sm-3 col-md-2 col-lg-2" })
                <div class="col-xs-12 col-sm-8 col-md-3 col-lg-3">
                    @Html.Partial("_MetodoResultadoInterfazMASED")*
                </div>
                @Html.Label("Nro Prococolo: ", new { @class = " -label col-xs-12 col-sm-3 col-md-2 col-lg-2" })
                <div class="col-xs-12 col-sm-8 col-md-3 col-lg-3">
                    @Html.TextBox("nroProtocolo", (string)ViewBag.nroProtocolo, new { @class = "form-control" })
                </div>*
            </div>
            <br />
            <div class="row">
                <div class="col-xs-4">
                </div>
                <div class="col-xs-4">
                    <button id="btnValidar" name="btnValidar" class="btn btn-primary pull-middle" onclick="ExportToTable()" title="">
                        <span class="glyphicon glyphicon-registration-mark "></span> VALIDAR MUESTRAS
                    </button>
                </div>
                <div class="col-xs-4">
                </div>
            </div>
        </div>
    </div>
</fieldset>
<br />
<div>
    <table id="tblData" class="table table-hover table-striped">
        <thead>
            <tr>
                <th>
                    #
                </th>
                <th>
                    Nro Protocolo
                </th>
                <th>
                    Muestra
                </th>
                <th>
                    Kit/Metodo
                </th>
                <th>
                    Muestra Sin Recepcionar
                </th>
                <th>
                    Muestra Con Resultado
                </th>
            </tr>
        </thead>
        <tbody id="filaMuestra">
        </tbody>
    </table>
</div>
<div class="row">
    <button id="btnLimpiar" name="btnLimpiar" class="btn btn-primary pull-left" onclick="Limpiar()" title="">
        <span class="glyphicon glyphicon-minus-sign"></span> Limpiar
    </button>
    <button id="btnProcesar" name="btnProcesar" class="btn btn-primary pull-right" onclick="ProcesarMuestras()" title="">
        <span class="glyphicon glyphicon-save"></span> Confirmar
    </button>
</div>